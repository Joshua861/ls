let x := sqrt(17) * 44.5 / 10;
let y := rand() + rand() * 3 + max(rand_between(5, 10), 7);
let z := 100 * x / y;

print(join_array([x,y,z], " | "));

let apple := 5.5 == 5.5;
let bee := 5.5 != 123;
let cider := !(apple && bee);
let dreams := cider ^ bee;
let eepy := 10 > 5;
let fidget := rand() > 0.5;

print(join_array(["apple =", apple, "\nbee =", bee, "\ncider =", cider, "\ndreams =", dreams, "\neepy =", eepy, "\nfidget =", fidget], " "));

